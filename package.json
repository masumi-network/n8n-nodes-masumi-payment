{
    "name": "n8n-nodes-masumi-payment",
    "version": "0.5.12",
    "description": "Masumi Payment nodes for n8n - webhook trigger, processor, and respond handler",
    "keywords": [
        "n8n-community-node-package",
        "masumi",
        "paywall",
        "trigger",
        "cardano",
        "blockchain",
        "cryptocurrency",
        "payment",
        "webhook"
    ],
    "license": "MIT",
    "homepage": "https://www.masumi.network/",
    "author": {
        "name": "Masumi Network",
        "email": "plan-net@house-of-communication.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/masumi-network/n8n-nodes-masumi-payment.git"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "packageManager": "npm@10.0.0",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc && gulp build:icons",
        "dev": "tsc --watch",
        "format": "prettier nodes credentials --write",
        "lint": "eslint nodes/**/*.ts credentials/**/*.ts package.json",
        "lintfix": "eslint nodes/**/*.ts credentials/**/*.ts package.json --fix",
        "prepublishOnly": "npm run build && npm run lint -q && npm run format && npm run test",
        "test": "jest",
        "test:integration": "RUN_INTEGRATION_TESTS=true jest nodes/MasumiPaywall/integration.test.ts",
        "test:manual": "npm run build && node test-integration-manual.js"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "dist/credentials/MasumiPaywallApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/MasumiPaywall/MasumiPaywall.node.js",
            "dist/nodes/MasumiPaywallTrigger/MasumiPaywallTrigger.node.js",
            "dist/nodes/MasumiPaywallRespond/MasumiPaywallRespond.node.js"
        ]
    },
    "devDependencies": {
        "@types/jest": "^29.5.5",
        "@types/node": "^20.8.0",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.7.5",
        "eslint": "^8.51.0",
        "eslint-plugin-n8n-nodes-base": "^1.16.1",
        "gulp": "^4.0.2",
        "jest": "^29.7.0",
        "n8n-workflow": "^1.104.0",
        "prettier": "^3.0.3",
        "ts-jest": "^29.4.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.2.2"
    },
    "peerDependencies": {
        "n8n-workflow": "^1.104.0"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "moduleNameMapper": {
            "^../../../package.json$": "<rootDir>/package.json"
        },
        "roots": [
            "<rootDir>/nodes",
            "<rootDir>/credentials"
        ]
    },
    "dependencies": {
        "axios": "^1.11.0",
        "dotenv": "^17.2.1"
    }
}
